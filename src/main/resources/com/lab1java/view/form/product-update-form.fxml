<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.lab1java.controller.form.ProductUpdateFormController">
    <children>
        <Pane prefHeight="475.0" prefWidth="300.0">
            <children>
                <TextField disable="true" fx:id="name" layoutX="76.0" layoutY="63.0" />
                <TextField fx:id="type" layoutX="76.0" layoutY="104.0" />
                <TextField fx:id="price" layoutX="76.0" layoutY="145.0" />
                <TextArea fx:id="description" layoutX="50.0" layoutY="186.0" prefHeight="200.0" prefWidth="200.0" />
                <Text layoutX="134.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
                <Text layoutX="137.0" layoutY="101.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Type" />
                <Text layoutX="137.0" layoutY="142.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Price" />
                <Text layoutX="120.0" layoutY="183.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description" />
                <Button layoutX="124.0" layoutY="405.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
                <Text layoutX="76.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Update a product">
                    <font>
                        <Font name="System Bold" size="16.0" />
                    </font>
                </Text>
            </children>
        </Pane>
    </children>
</VBox>
