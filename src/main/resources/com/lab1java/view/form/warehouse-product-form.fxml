<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lab1java.controller.form.WarehouseProductFormController">
   <children>
      <Pane prefHeight="256.0" prefWidth="300.0">
         <children>
            <ChoiceBox fx:id="warehouse" layoutX="76.0" layoutY="63.0" prefWidth="150.0" />
            <Text layoutX="76.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Warehouse" />
            <ChoiceBox fx:id="product" layoutX="76.0" layoutY="105.0" prefWidth="150.0" />
            <Text layoutX="76.0" layoutY="101.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Product" />
            <Button layoutX="124.0" layoutY="184.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
            <Text layoutX="76.0" layoutY="143.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Amount" />
            <Spinner fx:id="amount" initialValue="1" layoutX="77.0" layoutY="147.0" max="99" min="1" />
            <Text layoutX="60.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add warehouse product">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Text>
         </children>
      </Pane>
   </children>
</VBox>
